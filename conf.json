{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": ["./src","./README.md"],
    "includePattern": ".+\\.js$",
    "excludePattern": "(docs)"
  },
  "plugins": [],
  "templates": {
    "sort"                  : true,
    "systemName"            : "KkmServer API",
    "copyright"             : "&copy; 2017 Oleg Muraveyko  <a href='//jskkmserver.ru'>jskkmserver.ru</a>",
    "includeDate"           : false,
    "navType"               : "vertical",
    "theme"                 : "cerulean",
    "linenums"              : true,
    "methodHeadingReturns"  : false
  },
  "markdown": {
    "parser": "gfm",
    "hardwrap": true
  },
  "opts": {
    "template": "../../landing/ink-docstrap/template",
    "encoding": "utf8",
    "destination": "docs/",
    "recurse": true,
    "tutorials": "./example",
    "private": false
  }
}